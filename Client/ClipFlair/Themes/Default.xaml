<ResourceDictionary
 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
 xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk"    
 xmlns:local="clr-namespace:FlipPanel;assembly=FlipPanel"
  >
  <!-- xmlns:conv="clr-namespace:Converters;assembly=Converters" -->

  <Style x:Key="PropertyLabelStyle" TargetType="sdk:Label">
    <Setter Property="FontWeight" Value="SemiBold" />
    <Setter Property="Margin" Value="5,0,5,0" />
  </Style>

  <Style x:Key="PropertyStackPanelStyle" TargetType="StackPanel">
    <Setter Property="Margin" Value="0,5,0,5" />
    <Setter Property="HorizontalAlignment" Value="Stretch" />
  </Style>
  
  <!-- <conv:VisibilityToBooleanConverter x:Key="VisibilityToBoolean" /> -->

</ResourceDictionary>
